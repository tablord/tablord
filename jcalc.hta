<H1>StateMachine</H1><PRE onclick=jc.selectElement(this); id=code0401 class=CODE contentEditable=false jQuery15109145439506177564="12" jQuery15105776228114506182="12" jQuery15105482577185445079="12" jQuery151009513562338411474="12" jQuery15109029346241980511="12">v('count',-1);
stateMachine('myStateMachine')
.state('state1').initial()
  .run('v("count",count+1)')
  .transition('state2','count == 10')
.state('state2')
  .run('v("count",count-1)')
  .transition('state1','count == 0')
.end();
  </PRE>
<DIV onclick=jc.outClick(this) id=out0401 class="OUTPUT SUCCESS">
<DIV class=STATEMACHINE>myStateMachine
<DIV class="SMSTATE SMCURRENTSTATE">state1<BR>run : <SPAN class=CODEVIEW>v("count",count+1)</SPAN>
